def solution(s):
    answer = 0

    for i in range(len(s)):
        s = s[1:] + s[0]
        stack = []
        for j in range(len(s)):
            if not stack:   # 비어있을 때 (else를 여기에 포함하고 싶다...)
                stack.append(s[j])
            elif stack[-1] == "(" and s[j] == ")":
                stack.pop()
            elif stack[-1] == "{" and s[j] == "}":
                stack.pop()
            elif stack[-1] == "[" and s[j] == "]":
                stack.pop()
            else:  
                stack.append(s[j])
        if not stack:
            answer += 1
            
    return answer
