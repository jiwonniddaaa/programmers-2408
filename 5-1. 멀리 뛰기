def solution(n):
    # a = 0   # 1의 개수
    # b = 0   # 2의 개수
    
    # 팩토리얼 계산 함수
    def factorial(x):
        result = 1
        for i in range(2, x+1):
            result *= i
        return result
    
    # 중복조합 계산 공식 
    answer = 0
    for a in range(n, -1, -2):
        b = (n - a)//2  # b += 1
        answer += factorial(a+b)//(factorial(a)*factorial(b))
        
    
    return answer % 1234567



# 7
# 1*7  ->  1
# 1*5 + 2  ->  6
# 1*3 + 2*2  ->  5!/(3!*2!) = 10
# 1*1 + 2*3  ->  4

# 6
# 1*6  ->  1
# 1*4 + 2  ->  5 
# 1*2 + 2*2  ->  6 = 4!/(2!*2!)
# 2*3  ->  1

