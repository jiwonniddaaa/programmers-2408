### for문 2번으로 수정 ###
def solution(elements):
    new_set = set()   # set : 중복 방지 
    n = len(elements)
    
    for i in range(1, n+1):   # 길이가 i인 부분 수열
        for j in range(n):   # 시작점 : j
            l = i + j 
            if l >= n: 
                lst = elements[j:] + elements[:l-n]
            else:
                lst = elements[j:l]
            new_set.add(sum(lst))
            
    return len(new_set) 


### for문 3번 돌기 : 시간 초과 ###
def solution(elements):
    lst = set()   # set : 중복 방지 
    n = len(elements)
    
    for i in range(1, n+1):   # 길이가 i인 부분 수열
        for j in range(n):   # 시작점 : j
            answer = 0
            for k in range(i):
                l = k + j 
                if l >= n: l -= n
                answer += elements[l]
            lst.add(answer)
            
    return len(lst) 
