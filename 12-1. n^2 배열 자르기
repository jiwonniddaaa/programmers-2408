def solution(n, left, right):
    array = []
    
    for i in range(left, right+1):
        row = i // n + 1
        column = i % n + 1
        array.append(max (row, column))
    
    return array



### 2차원 배열 생성 -> 1차원으로 변경 후 슬라이싱 : 시간 초과 ###

def solution(n, left, right):
    # array = [[0 for i in range(n)] for i in range(n)]
    array = [[0] * n for i in range(n)]
    
    for i in range(n):
        for j in range(n):
            if i <= j:
                array[i][j] = j+1
            else:
                array[i][j] = i+1
    lst = sum(array, [])
    
    return lst[left:right+1]
