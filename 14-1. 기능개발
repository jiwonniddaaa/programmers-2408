def solution(progresses, speeds):
    times = []
    answer = []
    
    for i in range(len(progresses)):
        num = 100 - progresses[i]
        if num % speeds[i] == 0:
            times.append(num//speeds[i])
        else:
            times.append(num//speeds[i] + 1)
    
    maxtime = times[0]
    count = 1
    
    for i in range(1, len(times)):
        if times[i] > maxtime:
            answer.append(count)
            count = 1
            maxtime = times[i]
        else: count += 1
    if times:
        answer.append(count)

    # 이건 왜 안될까
    # i = 0
    # for j in range(len(times)-1):
    #     if times[i] < times[i+1]:
    #         answer.append(i+1)
    #         times = times[i+1:]
    #         i = 0
    #     else:
    #         i += 1
    # if times:
    #     answer.append(len(times))
            
    return answer
