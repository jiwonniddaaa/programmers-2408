# for문으로 구현
def solution(n):
    fibo = list(range(n+1))
    
    for i in range(1, n+2):
        if i == 1:
            fibo[i-1] = 0
        elif i == 2:
            fibo[i-1] = 1
        else:
            fibo[i-1] = fibo[i-2] + fibo[i-3]
                
    answer = fibo[n] % 1234567
            
    return answer 



# 재귀함수 쓴 경우 (런타임 에러)
def solution(n):   
    def fibo(n):
        if n == 0:
            return 0
        elif n == 1:
            return 1
        else:
            return fibo(n-1) + fibo(n-2)
    return fibo(n) % 1234567

